doctype html
html(lang='en', ng-app='pm')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title PartsMart
    // Bootstrap Core CSS
    link(href='/assets/css/bootstrap.min.css', rel='stylesheet')
    // Main stylesheet
    link(href='/assets/css/hallooou.css', rel='stylesheet')
    // Color stylesheet
    link(href='/assets/css/colors.css', rel='stylesheet')
    link(href='/assets/css/colors_2.css?v=1.1', rel='stylesheet')
    // Plugin stylesheets
    link(href='/assets/css/plugins/owl.carousel.css', rel='stylesheet')
    link(href='/assets/css/plugins/owl.theme.css', rel='stylesheet')
    link(href='/assets/css/plugins/owl.transitions.css', rel='stylesheet')
    link(href='/assets/css/plugins/animate.css', rel='stylesheet')
    link(href='/assets/css/plugins/magnific-popup.css', rel='stylesheet')
    link(href='/assets/css/plugins/jquery.mb.YTPlayer.min.css', rel='stylesheet')
    // Custom Fonts
    link(href='/assets/css/font-awesome.min.css', rel='stylesheet')
    link(href='http://fonts.googleapis.com/css?family=Raleway:100,600', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:300', rel='stylesheet', type='text/css')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
      script.
        document.createElement('video');
  body#home.parallax-page
    // Navigation
    nav.navbar.navbar-custom.navbar-fixed-top(role='navigation')
      .container
        .navbar-header.pull-left
          a.navbar-brand.page-scroll(href='#page-top')
            // replace with your brand logo/text
            span.brand-logo
              img.img-responsive(src='/assets/images/logo.png', style='max-height: 64px;', alt='PartsMart', title='PartsMart')
        .main-nav.pull-right
          .button_container.toggle
            span.top
            span.middle
            span.bottom
        #overlay.overlay
          nav.overlay-menu
            ul
              li
                a(href='#about') About PartsMart
              li
                a(href='#products') Why Choose Us
              li
                a(href='#portfolio') What We Do
              li
                a(href='#contact') Contact Us
      // /.container

    block content
     
    // Contact section
    section#contact.contact.content-section.no-bottom-pad
      .container
        .row.text-center
          .col-md-12
            h2 Contact
            h3.caption(style='color:black;')
              | Feel free to get in touch with us if you have any questions
          // /.col-md-12
        // /.row
      // /.container
      .container
        .row.form-container
          .col-md-8.contact-form
            h3 Drop us a line
            form#contactForm.ajax-form(method='post', action='/touch')
              .form-group
                input#name.form-control(type='text', name='name', placeholder='Your Name...', value='', required='')
              .form-group
                input#email.form-control(type='email', name='email', placeholder='Your email...', value='', required='')
              .form-group
                input#phone.form-control(type='phone', name='phone', placeholder='Your phone...', value='', required='')
              .form-group
                textarea.form-control(rows='4', name='message', placeholder='Your message...', required='')
              .form-group
                button.btn.btn-default(type='submit', name='submit')
                  i.fa.fa-paper-plane.fa-fw
                  |  Send
          // /.contact-form

          .col-md-4.contact-address
            h3 Our address
            p(style='color:black;')
              | 41 Saka Tinubu Street,
              br
              | Victoria Island, Lagos,
              br
              |  NIGERIA.
            ul
              li(style='color:black;')
                span Email:
                | info@partsmart.com
              li(style='color:black;')
                span Phone:
                | +234 1 342 6989
          // /.contact-address
        // /.row
      // /.container
      .container-fluid.buffer-forty-top
        .row
          section(id='cd-google-map no-bottom-pad')
            #google-container
            #cd-zoom-in
            #cd-zoom-out
        // /.row
      // /.container-fluid
      // Footer
      footer(style='background-color:#1a133f;')
        .container
          .row.text-center
            .col-md-12.segment
              a(href='#', title='PartsMart')
                h2
                  img.img-responsive(src='/assets/images/logo.png', alt='PartsMart', title='PartsMart', style='max-height: 128px;')
              p.white  Genuine automobile parts, fast &amp; affordable
            // /.col-md-12
          // /.row
          .row.text-center
            .col-md-12.social.segment
              h4 Stay Connected
              a(href='#')
                i.fa.fa-facebook.fa-3x
              a(href='#')
                i.fa.fa-twitter.fa-3x
              a(href='#')
                i.fa.fa-linkedin.fa-3x
            // /.col-md-12
          // /.row
        // /.container
        .copynote(style='background-color:#f5c607;color:#1a133f;')
          .container
            .row
              .col-md-12.text-center
                | Â© 2017 &nbsp; PartsMart
              // /.col-md-12
            // /.row
          // /.container
        // /.copynote
        .nav.pull-right.scroll-top
          a(href='#home', title='Scroll to top')
            i.fa.fa-angle-up
      // /.footer
      // jQuery
      script(src='/assets/js/jquery.min.js')
      // Bootstrap Core JavaScript
      script(src='/assets/js/bootstrap.min.js')
      // Plugin JavaScript
      script(src='/assets/js/plugins/wow.min.js')
      script(src='/assets/js/plugins/owl.carousel.min.js')
      script(src='/assets/js/plugins/jquery.parallax-1.1.3.js')
      script(src='/assets/js/plugins/jquery.magnific-popup.min.js')
      script(src='/assets/js/plugins/jquery.mb.YTPlayer.min.js')
      script(src='/assets/js/plugins/jquery.countTo.js')
      script(src='/assets/js/plugins/jquery.inview.min.js')
      script(src='/assets/js/plugins/pace.min.js')
      script(src='/assets/js/plugins/jquery.easing.min.js')
      script(src='/assets/js/plugins/jquery.validate.min.js')
      script(src='/assets/js/plugins/additional-methods.min.js') 
      script(src='/node_modules/angular/angular.min.js')
      script(src='/node_modules/angular-resource/angular-resource.min.js')
      script(src='/node_modules/angular-route/angular-route.min.js')
      script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyBoVKfEihX__NdMwdDysA6Vve6PE9Ierj4')
      // Custom JavaScript
      script(src='/assets/js/hallooou.js')
      script(src='/client/client.js')
