extends layout.site

block content
  // Intro Header
  // Full Page Image Background Carousel Header
  header#intro-carousel.carousel.slide
    // Optional Indicators
    //
      <ol class="carousel-indicators">
      <li data-target="#intro-carousel" data-slide-to="0" class="active"></li>
      <li data-target="#intro-carousel" data-slide-to="1"></li>
      <li data-target="#intro-carousel" data-slide-to="2"></li>
      </ol>
    // Wrapper for slides
    .carousel-inner
      .item.active
        // Set the first background image using inline CSS below.
        .fill(style="background-image:url('/assets/images/cover-one.jpg');")
        .carousel-caption
          h1.animated.slideInDown
            | Welcome to PartsMart
            span.highlight 
          p.intro-text.animated.slideInUp
            | Genuine automobile parts, fast &amp; affordable
          a.btn.btn-default.btn-lg(href='/search') Search
        .overlay-detail
      // /.item
      .item
        // Set the second background image using inline CSS below.
        .fill(style="background-image:url('/assets/images/cover-two.jpg');")
        .carousel-caption
          h1.animated.slideInDown
            span.highlight Responsive & Retina
            |  Ready
          p.intro-text.animated.slideInUp A clean and bold HTML5 template, Halloou is ultra responsive and retina ready.
          a.btn.btn-default.btn-lg(href='http://bit.ly/hh5_template') Download theme
        .overlay-detail
      // /.item
      .item
        // Set the second background image using inline CSS below.
        .fill(style="background-image:url('/assets/images/cover-three.jpg');")
        .carousel-caption
          h1.animated.slideInDown
            | Customizable 
            span.highlight colors
            |  & blocks
          p.intro-text.animated.slideInUp
            | Halloou gives you the freedom to easily change the color of the whole template to match your brand, re-arrange blocks to suit your needs.
          a.btn.btn-default.btn-lg(href='http://bit.ly/hh5_template') Download theme
        .overlay-detail
      // /.item
    // Controls
    a.left.carousel-control.squared(href='#intro-carousel', data-slide='prev')
      i.fa.fa-angle-left.fa-2x
    a.right.carousel-control.squared(href='#intro-carousel', data-slide='next')
      i.fa.fa-angle-right.fa-2x
    .mouse
